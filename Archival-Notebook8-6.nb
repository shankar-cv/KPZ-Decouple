Cell[TextData[{
 "For a decoupled tensor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SubscriptBox["\[Beta]", "1"]}], TraditionalForm]],ExpressionUUID->
  "d779a336-0602-46a2-aae7-c81d231f0b74"],
